{
  "name": "@esengine/ai",
  "version": "2.0.0",
  "description": "TypeScript AI modules for game development: BehaviourTree, FSM, and UtilityAI",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./behaviour-tree": {
      "import": "./dist/behaviourTree/index.js",
      "types": "./dist/behaviourTree/index.d.ts"
    },
    "./fsm": {
      "import": "./dist/fsm/index.js",
      "types": "./dist/fsm/index.d.ts"
    },
    "./utility-ai": {
      "import": "./dist/utilityAI/index.js",
      "types": "./dist/utilityAI/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "test": "echo \"No tests specified\" && exit 0",
    "prepublishOnly": "npm run clean && npm run build",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "typescript",
    "ai",
    "behaviour-tree",
    "fsm",
    "utility-ai",
    "game-development",
    "cocos",
    "laya",
    "mini-game"
  ],
  "author": "esengine",
  "license": "MIT",
  "devDependencies": {
    "rimraf": "^5.0.5",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/esengine/BehaviourTree-ai.git"
  },
  "bugs": {
    "url": "https://github.com/esengine/BehaviourTree-ai/issues"
  },
  "homepage": "https://github.com/esengine/BehaviourTree-ai#readme"
}
